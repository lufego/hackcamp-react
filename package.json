{
  "name": "hackcamp.react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "axios-mock-adapter": "^1.9.0",
    "bootstrap": "^4.0.0-beta",
    "deep-freeze-strict": "^1.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-form": "^7.0.4",
    "redux-thunk": "^2.2.0",
    "remove-trailing-separator": "^1.0.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --bracket-spacing=false",
      "eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "prettier": "^1.4.4",
    "react-scripts": "1.0.13"
  },
  "scripts": {
    "lint": "node_modules/eslint/bin/eslint.js src/App.js",
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:middlewares:1": "yarn test src/__tests__/middlewares_1",
    "test:middlewares:2": "yarn test src/__tests__/middlewares_2",
    "test:reducers:movies": "yarn test src/__tests__/reducers/movies.specs.js",
    "test:reducers:comments": "yarn test src/__tests__/reducers/comments.specs.js",
    "test:actions:movies": "yarn test src/__tests__/actions/movies.specs.js",
    "test:actions:comments": "yarn test src/__tests__/actions/comments.specs.js",
    "eject": "react-scripts eject"
  }
}
